<!DOCTYPE html>
<html lang="es-es">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>ABC Bank</title>
    <base href='@Url.Content("~/")'>

    <!--STYLESHEET-->
    <!--Bootstrap Stylesheet-->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <!--Nifty Stylesheet-->
    <link href="~/Content/css/nifty.min.css" rel="stylesheet">
    <!--ICONOS-->
    <link href='~/Content/css/openSans.css' rel='stylesheet' type='text/css'>
    <link href="~/Content/css/demo/nifty-demo-icons.min.css" rel="stylesheet">
    <link href="~/Content/plugins/themify-icons/themify-icons.min.css" rel="stylesheet">
    <link href="~/Content/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!--Pace - Page Load Progress Par-->
    <link href="~/Content/plugins/pace/pace.min.css" rel="stylesheet">
    <script src="~/Content/plugins/pace/pace.min.js"></script>
    <link href="~/Content/css/demo/nifty-demo.min.css" rel="stylesheet">
    <link href="~/Content/plugins/animate-css/animate.min.css" rel="stylesheet">
    <!--DataTables-->
    <link href="~/Content/plugins/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/plugins/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <!--Select2-->
    <link href="~/Content/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" />
    <!--DateRangePicker-->
    <link href="~/Content/css/daterangepicker.css" rel="stylesheet" type="text/css" />

    <!--CSS [ SECCION ]-->
    @RenderSection("styles", required: false)

</head>
<body>
    <div id="container" class="effect aside-float aside-bright">
        <!--NAVBAR-->
        <header id="navbar">
            <h1 style="text-align:center">ABC Bank</h1>
        </header>
        <!--END NAVBAR-->
        <div class="boxed">
            <!--CONTENT CONTAINER-->
            <div id="content-container">

                <!--Page content-->
                <div id="page-content">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="panel panel-body">
                                <div class="panel-body">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--End page content-->
            </div>
            <!--END CONTENT CONTAINER-->

        </div>

        <!-- FOOTER -->
        <footer id="footer">
            <p class="pad-lft">&copy; @DateTime.Now.Year - ABC Bank</p>
        </footer>
        <!-- END FOOTER -->
    </div>

    <!-- END OF CONTAINER -->
    <!--JAVASCRIPT-->
    <!--jQuery-->
    <script src="~/Scripts/template/jquery.min.js"></script>
    <!--BootstrapJS-->
    <script src="~/Scripts/template/bootstrap.min.js"></script>
    <!--NiftyJS-->
    <script src="~/Scripts/template/nifty.min.js"></script>
    <!--DataTables-->
    <script src="~/Content/plugins/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/plugins/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/Content/plugins/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/Content/plugins/jszip/dist/jszip.min.js"></script>
    <!--Select2-->
    <script src="~/Content/plugins/select2/js/select2.min.js"></script>
    <!--DateRangePicker-->
    <script src="~/Scripts/template/moment.min.js"></script>
    <script src="~/Scripts/template/daterangepicker.js"></script>
    <script src="~/Content/js/demo/ui-modals.js"></script>

    <!--SectionScripts -->
    @RenderSection("script", required: false)

</body>
</html>
